<div class="w-25 mx-auto mt-4">

<%= simple_form_for([:owner, @party_places]) do |f| %>

  <%= f.input :name %>
  <%= f.input :address %>
  <%= f.input :description %>
  <%= f.input :price_per_day %>
  <%= f.input :picture %>

  <%= f.button :submit, "Créer un lieu de fête", class: "btn-flat" %>

<% end %>
</div>
